<p id="notice"><%= notice %></p>

<h1>Categories</h1>

<section class="body-text">
<ul>
  <% @categories.each do |item| %>
  <li><%= link_to (item.name),  category_path(item) %></li>
  <% end %>
</ul>

<%#render partial: '/application/index', locals: {collection: @categories, field:'name'}%>
</section>
