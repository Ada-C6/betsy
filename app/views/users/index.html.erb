<!-- Browse products by merchant (users) (users#index â‡’ index view, shows all sellers/merchants) -->
<h1>All Merchants And Their Amazing Products</h1>

<% @users.each do |user| %>
<h2><%= link_to "#{user.user_name}", search_sellers_products_path(user) %></h2>
<ul>Products:
<% user.products.each do |product| %>
 <li><%= link_to "#{product.name}", product_path(product) %></li>
 <% end %>
 </ul>
<% end %>
