<h1>All Products Here</h1>

<%@product.each do |product|%>
  <section>
    <h2>
      <%= product.name %>
    </h2>
    <p>
      <%= image_tag "#{product.photo_url}"%>
    </p>
    <p>
      <%= product.description %>
    </p>

    <h3>
      Categories
    </h3>

    <ul>
    <% product.categories.each do |category| %>
      <li><%= category.name %></li>
      <% end %>
    </ul>
</section>
<%end%>
