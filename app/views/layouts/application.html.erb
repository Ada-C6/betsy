<!DOCTYPE html>
<html>
<head>
  <title>Betsy</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
  <header>
    <h1><%= link_to "Unicorns 'R' Us", root_path %></li></h1>
    <nav>
      <ul>
        <li><%= link_to "Browse by store", stores_index_path %></li>
        <li><%= link_to "Browse by category", categories_index_path %></li>
        <li><%= link_to "Browse all", homepages_show_path %></li>
            <% if session[:user_id] == nil %>
        <li><%= link_to "Merchant Login", "/auth/github" %></li>
        <% else %>
        <li><%= link_to "Logout", session_delete_path(session[:user_id]), method: :delete, data: {confirm:"End your session?"} %></li>
        <li><%= link_to "Merchant Home", "#" %>
        <% end %></li>
        <li><%=link_to image_tag("cart.png"), "#" %></li>
      </ul>
    </nav>
  </header>

  <%= yield %>

</body>
</html>
